
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Open Schooling</title>

    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
    <script src="~/Design/SweetAlert/sweetalert2.js"></script>
    <link href="~/Design/SweetAlert/SweetAlert2.css" rel="stylesheet" />
    <script src="~/Design/JS/Project.js"></script>
    <link rel="icon" href="~/MSBSHSE-LOGO.ico" />

    <script src="~/Scripts/bootstrap.min.js"></script>
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />"


    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js"></script>

    <link href="~/Design/CSS/grid.css" rel="stylesheet" />
    <style>
        .container {
            margin-right: 5px;
            margin-left: 5px;
            margin-bottom: 190px;
            background-color: white;
            width: auto;
        }

        .navbar-inverse {
            background-color: #3070ae;
        }

            .navbar-inverse .navbar-nav > li > a {
                color: white;
            }

        ul {
            margin: 0px;
            padding: 0px;
            background: #3070ae;
            list-style-type: none;
            position: relative;
        }


            ul li a {
                padding: 15px;
                color: #292929;
                text-decoration: none;
                font-size: 16px;
                display: block;
            }

            ul li:hover {
                background: #3070ae;
            }

            ul ul {
                position: absolute;
                width: min-content;
                display: none;
                border: 5px;
            }

                ul ul li {
                    display: block;
                    border: 5px;
                    border-color: red;
                }

            ul li:hover ul {
                z-index: 1;
                display: block;
                background-color: #3070ae;
            }

            ul ul li a {
                color: white;
                background-color: #3070ae;
                padding: 15px 25px;
            }

                ul ul li a:hover {
                    color: white;
                    text-decoration: none;
                }

        .kk {
            width: 150px;
        }

        .k2 {
            width: 220px;
        }

        a:hover {
            background-color: lightcoral;
            transition: 0.7s;
        }
    </style>
</head>
<body style="padding-top: 10px; margin: 0px 8px 0px 8px; background: #06213F ">

    <div style="background-color:white;height:100%;">
        <div class="container-fluid p-10  text-white text-center" style="background-color:white;">
            <div class="row">
                <div class="col-sm-12" align="center">
                    <div class="row w-100 mx-auto">
                        <div class=" col-10 px-md-0 text-center heading-name">
                            <img src="~/Design/Images/MSBSHSE-LOGO.png" style="float:left;width:8%" />
                            <h2>
                                Maharashtra State Board of Secondary & Higher Secondary Education Pune 411004
                            </h2>
                            <h3>
                                महाराष्ट्र राज्य मुक्त विद्यालय मंडळ, पुणे-४११००४

                            </h3>
                            <h3>  Official Website For Maharashtra State Board of Open Schooling (MSBOS)</h3>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="navbar navbar-inverse" style="margin-bottom:0px">
        <div class="container1">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li class="nav-item">
                        <a class="nav-link active" aria-current="page" href="../Home/Index">Home</a>

                    </li>



                    <li class="nav-item">
                        <a class="nav-link active" aria-current="page" href="../Home/Registration"> Student Registration</a>

                    </li>





                    <li class="nav-item">
                        <a href="#"> Login </a>
                        <ul>
                            <li><a class="nav-link active kk" href="../Home/Student_Login">Student Login</a></li>
                            <li><a class="nav-link active kk" href="../State/StateLogin">State Login</a></li>
                            <li><a class="nav-link active kk" href="../CenterLogin/CenterLogin">Center Login</a></li>
                            <li><a class="nav-link active kk" href="../Admin/Admin_Login">Admin Login</a></li>

                        </ul>

                    </li>

                    <li class="nav-item">
                        <a href="#"> सुचना/माहिती </a>
                        <ul>
                            <li><a target="_blank" <a class="nav-link active k2" href="~/Design/GR/Updated_Center_List.pdf" >Center List</a></li>
                            <li><a target="_blank" href="~/Design/GR/दिव्यांग विद्यार्थी मुल्यांकन.pdf">दिव्यांग विद्यार्थी मुल्यांकन</a></li>
                            <li><a target="_blank" href="~/Design/GR/माहितीपुस्तिका.pdf">माहितीपुस्तिका</a></li>
                            <li><a target="_blank" href="~/Design/GR/राज्य मुक्त विद्यालय स्थापना.pdf">राज्य मुक्त विद्यालय स्थापना</a></li>
                            <li><a target="_blank" href="~/Design/GR/घडी_पुस्तिका.pdf">घडी पुस्तिका</a></li>

                        </ul>

                    </li>


                    <li> <a class="nav-link active" aria-current="page" href="../Result/ResultCredentials"> Result</a></li>
                </ul>








            </div>
        </div>
    </div>
    <div class="container body-content table-bordered">
        @RenderBody()
        <hr />
        <footer>
            <p>Copyright &copy; @DateTime.Now.Year -All Rights Reserved - Maharashtra State Board Pune</p>
        </footer>
    </div>



    <script src="~/Design/JS/ApplicationForm.js"></script>
    <script src="~/Design/JS/jquery.validate.min.js"></script>
    <script src="~/Design/JS/jquery.validate.unobtrusive.js"></script>
</body>
</html>


